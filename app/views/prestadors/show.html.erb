<section class="content-header">
    <%if notice%>
      <div class="alert alert-success" role="alert"><%= notice %></div>
      <%end%>
</section>

<!-- Main content -->
<section class="content">
 <!-- Small boxes (Stat box) -->
<div class="box box-primary">
   <div class="box-header with-border">
      <h3 class="box-title">Prestador</h3>
    </div><!-- /.box-header -->
<form role="form">
<div class="box-body">

<p>
    <strong>Categoria:</strong>
    <%= @prestador.categoriaprestador.nome_categoria %>
  </p>

<p>
  <strong>CPF/CNPJ:</strong>
  <%= @prestador.desc_cpfcnpj %>
</p>

<p>
  <strong>Nome prestador:</strong>
  <%= @prestador.nome_prestador %>
</p>

<p>
  <strong>Nome responsavel:</strong>
  <%= @prestador.nome_responsavel %>
</p>

<p>
  <strong>Observacao:</strong>
  <%= @prestador.observacao %>
</p>

 </div><!-- /.box -->
</form>
</div><!-- /.box-body -->

<div class="btn-group">
     <%= link_to edit_prestador_path(@prestador), :class => "btn btn-default btn-sm" do %>
  <span class="glyphicon glyphicon-pencil"></span> Editar
<% end %>
<%= link_to prestadors_path, :class => "btn btn-default btn-sm" do %>
   <span class="glyphicon glyphicon-arrow-left"></span> Voltar
<% end %>
</div>
