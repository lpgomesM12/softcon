<section class="content-header">
    <%if notice%>
      <div class="alert alert-success" role="alert"><%= notice %></div>
      <%end%>
</section>

<!-- Main content -->
<section class="content">
 <!-- Small boxes (Stat box) -->
<div class="box box-primary">
   <div class="box-header with-border">
      <h3 class="box-title">Morador</h3>
    </div><!-- /.box-header -->
<form role="form">
<div class="box-body">

<p>
  <strong>Numr agencia:</strong>
  <%= @contabank.numr_agencia %>
</p>

<p>
  <strong>Nome conta:</strong>
  <%= @contabank.numr_conta %>
</p>

<p>
  <strong>Nome banco:</strong>
  <%= @contabank.nome_banco %>
</p>

<p>
  <strong>Situacao:</strong>
  <%= @contabank.situacao %>
</p>


</div><!-- /.box -->
</form>
</div><!-- /.box-body -->

<div class="btn-group">
     <%= link_to edit_contabank_path(@contabank), :class => "btn btn-default btn-sm" do %>
  <span class="glyphicon glyphicon-pencil"></span> Editar
<% end %>
<%= link_to contabanks_path, :class => "btn btn-default btn-sm" do %>
   <span class="glyphicon glyphicon-arrow-left"></span> Voltar
<% end %>
</div>
