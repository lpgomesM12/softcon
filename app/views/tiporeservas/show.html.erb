<div class="box box-primary">
   <div class="box-header with-border">
      <h3 class="box-title">Local reserva</h3>
    </div><!-- /.box-header -->
<form role="form">
<div class="box-body">

<p>
  <strong>Desc tiporeserva:</strong>
  <%= @tiporeserva.desc_tiporeserva %>
</p>


</div><!-- /.box -->
</form>
</div><!-- /.box-body -->

<div class="btn-group">
 <%= link_to edit_tiporeserva_path(@tiporeserva), :class => "btn btn-default btn-sm" do %>
