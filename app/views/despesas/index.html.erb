<p id="notice"><%= notice %></p>

<h1>Listing Despesas</h1>

<table>
  <thead>
    <tr>
      <th>Valr despesa</th>
      <th>Numr qtdparcela</th>
      <th>Condominio</th>
      <th>Prestador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @despesas.each do |despesa| %>
      <tr>
        <td><%= despesa.valr_despesa %></td>
        <td><%= despesa.numr_qtdparcela %></td>
        <td><%= despesa.condominio %></td>
        <td><%= despesa.prestador %></td>
        <td><%= link_to 'Show', despesa %></td>
        <td><%= link_to 'Edit', edit_despesa_path(despesa) %></td>
        <td><%= link_to 'Destroy', despesa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Despesa', new_despesa_path %>
