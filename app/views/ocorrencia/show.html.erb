<section class="content-header">
    <%if notice%>
      <div class="alert alert-success" role="alert"><%= notice %></div>
      <%end%>
</section>

<!-- Main content -->
<section class="content">
 <!-- Small boxes (Stat box) -->
<div class="box box-primary">
   <div class="box-header with-border">
      <h3 class="box-title">Morador</h3>
    </div><!-- /.box-header -->
<form role="form">
<div class="box-body">

<p>
  <strong>Data ocorrencia:</strong>
  <%= @ocorrencium.created_at %>
</p>

<p>
  <strong>Desc ocorrencia:</strong>
  <%= @ocorrencium.desc_ocorrencia %>
</p>

<p>
  <strong>Data saida:</strong>
  <%= @ocorrencium.data_saida %>
</p>

<p>
  <strong>Qdt minutos:</strong>
  <%= @ocorrencium.qdt_minutos %>
</p>

<p>
  <strong>Identificacao:</strong>
  <%= @ocorrencium.identificacao.desc_nome %>
</p>

</div><!-- /.box -->
</form>
</div><!-- /.box-body -->

<div class="btn-group">
     <%= link_to edit_ocorrencium_path(@ocorrencium), :class => "btn btn-default btn-sm" do %>
  <span class="glyphicon glyphicon-pencil"></span> Editar
<% end %>
<%= link_to ocorrencia_path, :class => "btn btn-default btn-sm" do %>
   <span class="glyphicon glyphicon-arrow-left"></span> Voltar
<% end %>
</div>
