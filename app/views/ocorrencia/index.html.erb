<p id="notice"><%= notice %></p>

<h1>Listing Ocorrencia</h1>

<table>
  <thead>
    <tr>
      <th>Data ocorrencia</th>
      <th>Desc ocorrencia</th>
      <th>Data saida</th>
      <th>Qdt minutos</th>
      <th>Identificacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ocorrencia.each do |ocorrencium| %>
      <tr>
        <td><%= ocorrencium.data_ocorrencia %></td>
        <td><%= ocorrencium.desc_ocorrencia %></td>
        <td><%= ocorrencium.data_saida %></td>
        <td><%= ocorrencium.qdt_minutos %></td>
        <td><%= ocorrencium.identificacao %></td>
        <td><%= link_to 'Show', ocorrencium %></td>
        <td><%= link_to 'Edit', edit_ocorrencium_path(ocorrencium) %></td>
        <td><%= link_to 'Destroy', ocorrencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

