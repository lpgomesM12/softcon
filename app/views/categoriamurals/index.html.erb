<p id="notice"><%= notice %></p>

<h1>Listing Categoriamurals</h1>

<table>
  <thead>
    <tr>
      <th>Nome categoria</th>
      <th>Condiminio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categoriamurals.each do |categoriamural| %>
      <tr>
        <td><%= categoriamural.nome_categoria %></td>
        <td><%= categoriamural.condiminio %></td>
        <td><%= link_to 'Show', categoriamural %></td>
        <td><%= link_to 'Edit', edit_categoriamural_path(categoriamural) %></td>
        <td><%= link_to 'Destroy', categoriamural, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categoriamural', new_categoriamural_path %>


<section class="content">
    <div class="row">
      <div class="col-md-12">
        <div class="box box-success">
           <div class="box-header with-border">
             <h3 class="box-title">Visitors Report</h3>
             <div class="box-tools pull-right">
               <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
             </div>
           </div><!-- /.box-header -->
           <div class="box-body no-padding">
             <div class="row">
               <div class="col-md-12 col-sm-12">
                 <br><br>
               </div><!-- /.col -->
             </div><!-- /.row -->
           </div><!-- /.box-body -->
          </div><!-- /.box -->
        </div>
      </div>
  </section>
