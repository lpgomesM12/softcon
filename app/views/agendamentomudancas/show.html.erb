<section class="content-header">
      <!--Mensagens aqui-->
    <%if notice%>
      <div class="alert alert-success" role="alert"><%= notice %></div>
     <%end%>
</section>

<!-- Main content -->
<section class="content">
 <!-- Small boxes (Stat box) -->
<div class="box box-primary">
   <div class="box-header with-border">
      <h3 class="box-title">Agendamento Mudan√ßa</h3>
    </div><!-- /.box-header -->
<form role="form">
<div class="box-body">


<p>
  <strong>Nome:</strong>
  <%= @agendamentomudanca.nome %>
</p>

<p>
  <strong>Telefone:</strong>
  <%= @agendamentomudanca.fone %>
</p>

<p>
  <strong>Apartamento:</strong>
  <%= @agendamentomudanca.apartamento.nome %>
</p>

<p>
  <strong>Data agendamento:</strong>
  <%=@agendamentomudanca.data_agendamento.blank? ? '' : @agendamentomudanca.data_agendamento.strftime("%d/%m/%Y") %>
</p>

<p>
  <strong>Valor da Taxa:</strong>
  <%= @agendamentomudanca.valr_taxa %>
</p>

<p>
  <strong>Periodo:</strong>
  <%= @agendamentomudanca.periodo %>
</p>

<p>
  <strong>Flag entrada:</strong>
  <%= @agendamentomudanca.flag_entrada %>
</p>

<p>
  <strong>Flag saida:</strong>
  <%= @agendamentomudanca.flag_saida %>
</p>

<p>
  <strong>Observacao:</strong>
  <%= @agendamentomudanca.observacao %>
</p>

</div><!-- /.box -->
</form>
</div><!-- /.box-body -->

<div class="btn-group">
 <%= link_to edit_agendamentomudanca_path(@agendamentomudanca), :class => "btn btn-default btn-sm" do %>
<span class="glyphicon glyphicon-pencil"></span> Editar
<% end %>
<%= link_to agendamentomudancas_path, :class => "btn btn-default btn-sm" do %>
 <span class="glyphicon glyphicon-arrow-left"></span> Voltar
<% end %>
</div>
